{
  "manifest_version": 3,
  "name": "Gistify",
  "version": "1.0.0",
  "description": "Add a button to convert code blocks into GitHub Gists",
  "action": {
    "default_popup": "../index.html",
    "default_title": "Basic",
    "default_icon": {
      "16": "vhgb.png",
      "128": "vhgb.png"
    }
  },
  "host_permissions": [
    "https://apis.google.com/*"
  ],
  "permissions": [],
  "web_accessible_resources": [],
  "content_scripts": [],
  "background": {}
}
